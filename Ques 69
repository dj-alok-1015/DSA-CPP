int mySqrt(int x) {
        if(x<=1) return x;
        long long lo=0, hi=x;
        int ans=0;
        while(lo<=hi){
            long long mid=(lo+hi)/2;
            if(mid*mid==x) return mid;
            else if(mid*mid>x) hi= mid-1;
            else{
                ans=mid;//agr mid ke possible answer ho toh best option hota hai ushe ek variable bana k store kr lena
                lo=mid+1;
            }
        }
        return ans;
    }
